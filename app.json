{
  "expo": {
    "name": "Pecly",
    "slug": "Pecly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/icon.png"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.Pecly",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Nous avons besoin de votre localisation pour afficher les r√©sultats autour de vous.",
        "NSLocationAlwaysUsageDescription": "Nous avons besoin de votre localisation pour suivre votre position en continu."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "googleServicesFile": "./android/app/google-services.json"
      },
      "package": "com.anonymous.Pecly"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      [
        "expo-splash-screen",
        {
          "imageWidth": 200,
          "resizeMode": "contain",
          "image": "./assets/images/splashscreen_logo.png",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Pecly to use your location"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}